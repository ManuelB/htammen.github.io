<mvc:View xmlns:html="http://www.w3.org/1999/xhtml" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m"
	controllerName="de.tammenit.ui5.homepage.controller.Home">
	<App id="app">
		<pages>
			<Page title="My cool UI5 Homepage">
				<content>
					<TileContainer editable="false" allowAdd="false" id="__container0">
						<tiles>
							<CustomTile id="__tile1">
								<content>
									<Image width="194px" height="226px" id="__image0" src="https://avatars3.githubusercontent.com/u/847143"/>
								</content>
							</CustomTile>
							<StandardTile title="About me" info="Info" icon="sap-icon://customer" number="1" numberUnit="topic" 
								press="showAboutMe" removable="false"/>
							<StandardTile title="My projects" info="Info"
								icon="sap-icon://work-history" number="2" numberUnit="topic" removable="false"/>
						</tiles>
					</TileContainer>

				</content>
			</Page>
		</pages>
	</App>
</mvc:View>