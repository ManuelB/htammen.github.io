<forms:SimpleForm xmlns:forms="sap.ui.layout.form" xmlns:l="sap.ui.layout" xmlns="sap.m"
		labelSpanL="4"
		labelSpanM="4"
		labelSpanS="4"
		emptySpanL="0"
		emptySpanM="0"
		emptySpanS="0"
		maxContainerCols="2" layout="ResponsiveLayout"
		width="100%">
	<l:Grid content="{model>}" defaultSpan="L3 M6 S12">
		<l:content>
			<VBox alignItems="Start">
				<Label class="sapUiSmallMarginEnd" text="{model>Name}"/>
				<RatingIndicator enabled="false" value="{model>Rating}"/>
			</VBox>
		</l:content>
	</l:Grid>	
</forms:SimpleForm>
