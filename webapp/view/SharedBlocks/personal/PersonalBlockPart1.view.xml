<mvc:View xmlns:mvc="sap.ui.core.mvc" xmlns:core="sap.ui.core"
		  xmlns:layout="sap.ui.layout" xmlns:forms="sap.ui.layout.form" xmlns="sap.m">


	<forms:SimpleForm labelSpanL="4" labelSpanM="4"
					  labelSpanS="4" emptySpanL="0" emptySpanM="0" emptySpanS="0"
					  maxContainerCols="2" layout="ResponsiveLayout">
		<core:Title text="{i18n>view.profile.block.personalblockpart1.header}"/>

		<Label text="{i18n>view.profile.block.personalblockpart1.banktransfer}"/>

		<layout:VerticalLayout>
			<Text text="{i18n>view.profile.block.personalblockpart1.bankname}"/>
			<Text text="{i18n>view.profile.block.personalblockpart1.account}"/>
		</layout:VerticalLayout>
	</forms:SimpleForm>

</mvc:View>